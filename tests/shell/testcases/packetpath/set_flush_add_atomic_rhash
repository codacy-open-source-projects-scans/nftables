#!/bin/bash

. $NFT_TEST_BASEDIR/helpers/set_flush_add_atomic_helpers

tmp=$(mktemp)

cleanup()
{
	set_flush_add_atomic_cleanup "$tmp"
}

trap cleanup EXIT

set_flush_add_create_topo "rhash"

set_flush_add_atomic_run_test "$tmp" "ipv4_addr" "flags dynamic" "$ip_c, $ip_s" "ip saddr" || exit 1

exit 0
