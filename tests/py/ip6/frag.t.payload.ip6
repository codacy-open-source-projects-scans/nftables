# frag nexthdr tcp
ip6 test-ip6 output
  [ exthdr load ipv6 1b @ 44 + 0 => reg 1 ]
  [ cmp eq reg 1 0x06 ]

# frag nexthdr != icmp
ip6 test-ip6 output
  [ exthdr load ipv6 1b @ 44 + 0 => reg 1 ]
  [ cmp neq reg 1 0x01 ]

# frag nexthdr {esp, ah, comp, udp, udplite, tcp, dccp, sctp}
__set%d test-ip6 3
__set%d test-ip6 0
	element 32	element 33	element 6c	element 11	element 88	element 06	element 21	element 84
ip6 test-ip6 output
  [ exthdr load ipv6 1b @ 44 + 0 => reg 1 ]
  [ lookup reg 1 set __set%d ]

