# icmp type echo-request
inet test-inet output 
  [ meta load nfproto => reg 1 ]
  [ cmp eq reg 1 0x02 ]
  [ meta load l4proto => reg 1 ]
  [ cmp eq reg 1 0x01 ]
  [ payload load 1b @ transport header + 0 => reg 1 ]
  [ cmp eq reg 1 0x08 ]

# icmpv6 type echo-request
inet test-inet output 
  [ meta load nfproto => reg 1 ]
  [ cmp eq reg 1 0x0a ]
  [ meta load l4proto => reg 1 ]
  [ cmp eq reg 1 0x3a ]
  [ payload load 1b @ transport header + 0 => reg 1 ]
  [ cmp eq reg 1 0x80 ]

