ip test-ip input
  [ meta load l4proto => reg 1 ]
  [ cmp eq reg 1 0x06 ]
  [ payload load 2b @ transport header + 2 => reg 1 ]
  [ cmp eq reg 1 0x0016 ]
  [ meta load iiftype => reg 1 ]
  [ cmp eq reg 1 0x0001 ]
