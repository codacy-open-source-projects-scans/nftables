  [ cmp eq reg 1 0x62723000 0x00000000 0x00000000 0x00000000 ]

# meta ibrname "br0"
bridge test-bridge input
  [ meta load bri_iifname => reg 1 ]
  [ cmp eq reg 1 0x62723000 0x00000000 0x00000000 0x00000000 ]

# meta ibrvproto vlan
bridge test-bridge input
  [ meta load bri_iifvproto => reg 1 ]
  [ cmp eq reg 1 0x8100 ]

# meta ibrpvid 100
bridge test-bridge input
  [ meta load bri_iifpvid => reg 1 ]
  [ cmp eq reg 1 0x0064 ]

# meta protocol ip udp dport 67
bridge test-bridge input
